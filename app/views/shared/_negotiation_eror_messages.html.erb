<% if object.errors.any? %>
  <div id="error_explanation">
    <div class="alert alert-error">
    <p1 style="color:red;">  The Re-negotiation Contract contains <%= pluralize(object.errors.count, "error") %>. </p1>
    </div>
    <ul>
    <% object.errors.full_messages.each do |msg| %>
      <li>* <%= msg %></li>
    <% end %>
    </ul>
  </div>
<% end %>