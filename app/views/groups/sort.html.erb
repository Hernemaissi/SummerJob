<%= provide(:title, 'Add users') %>
<h1>All users</h1>
 <div id="content" style="height:100%;width:70%;float:right;">
   <ul class="users" id="sorttable">
     <%= render 'user_sort'  %>
   </ul>
 </div>

<%= render 'users/search_form' %>
<div id="quality_div" style="float:left; width:20%;">

  <% @qualities.each do |q| %>

  <%=  select("qualityvalue", "qualityvalue_id", q.qualityvalues.collect {|v| [ v.value, v.id ] }, { :prompt => q.title }, {:class => "quality_select"}) %>

  <% end %>

</div>

<div id="footer" style="clear:both;text-align:center;">
</div>
